{
  "name": "fold-to-spec",
  "displayName": "fold-to-spec",
  "description": "Fold only the specs of a multi-level RSpec file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fold-to-spec.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "generate-parser-wasm": "tree-sitter build-wasm node_modules/tree-sitter-ruby",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "tree-sitter-cli": "^0.20.8",
    "tree-sitter-ruby": "^0.19.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}
